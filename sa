        $(document).ready(function () {
            var cookieSettings = $.cookie("cookie");
            if (cookieSettings != null) {
                $("#cookie-popup").hide();
            }
        });

        function CookieSettings() {
            $.cookie("cookie", "true", {
                expires: 365
            }
            );
            $("#cookie-popup").hide();
        }
